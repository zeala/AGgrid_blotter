{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "typeRoots": [ "node_modules/@types" ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "build"
  ]
}
